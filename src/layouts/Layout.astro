---
import BaseHead from '@/components/BaseHead.astro'
import Footer from '@/components/Footer.astro'
import Header from '@/components/Header.astro'

export interface Props {
	title: string
	description: string
	image?: string
}

const { ...seo } = Astro.props
---

<html lang="en">
	<head>
		<BaseHead {...seo} />
	</head>
	<body>
		<Header />
		<main>
			<slot />
		</main>
		<Footer />
	</body>
</html>
