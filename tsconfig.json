{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"exactOptionalPropertyTypes": false,
		"strictNullChecks": true
	},
	"include": ["**/*.ts", "**/*.tsx", "**/*.astro", "**/*.svelte"]
}
